		      secondary_contact_job_title?,
		      secondary_contact_voice?,
		      secondary_contact_fax?,
		      secondary_contact_email?,
		      secondary_contact_web?)?,
		    notes? ) >
<!ATTLIST account	mode	(insert|update)	'insert'
			ignore 	(true|false)	'false'
			key	CDATA	#FIXED	'account_number' >

